local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Boom Click Mine", "BloodTheme")
local NUKE = Window:NewTab("NUKE")
local SettingsTab = Window:NewTab("Settings")
local CreditsTab = Window:NewTab("Credits")
local NUKE = NUKE:NewSection("NUKE")
local SettingSection = SettingsTab:NewSection("Change Settings")
local CreditsSection = CreditsTab:NewSection("Developers || Mds")

NUKE:NewTextBox("Auto Mine", "1-9", function(txt)
getgenv().digg = true
function dig(num)
while wait(0.001) do
if digg then
local args = {
    [1] = num,
    [2] = 1
}

game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Mine"):WaitForChild("HitBlock"):FireServer(unpack(args))
local args = {
    [1] = num,
    [2] = 2
}

game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Mine"):WaitForChild("HitBlock"):FireServer(unpack(args))
local args = {
    [1] = num,
    [2] = 3
}

game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Mine"):WaitForChild("HitBlock"):FireServer(unpack(args))
local args = {
    [1] = num,
    [2] = 4
}

game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Mine"):WaitForChild("HitBlock"):FireServer(unpack(args))
local args = {
    [1] = num,
    [2] = 5
}

game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Mine"):WaitForChild("HitBlock"):FireServer(unpack(args))
local args = {
    [1] = num,
    [2] = 6
}

game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Mine"):WaitForChild("HitBlock"):FireServer(unpack(args))
local args = {
    [1] = num,
    [2] = 7
}

game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Mine"):WaitForChild("HitBlock"):FireServer(unpack(args))
local args = {
    [1] = num,
    [2] = 8
}

game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Mine"):WaitForChild("HitBlock"):FireServer(unpack(args))
local args = {
    [1] = num,
    [2] = 9
}

game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Mine"):WaitForChild("HitBlock"):FireServer(unpack(args))
local args = {
    [1] = num,
    [2] = 10
}

game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Mine"):WaitForChild("HitBlock"):FireServer(unpack(args))
local args = {
    [1] = num,
    [2] = 11
}

game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Mine"):WaitForChild("HitBlock"):FireServer(unpack(args))
local args = {
    [1] = num,
    [2] = 12
}

game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Mine"):WaitForChild("HitBlock"):FireServer(unpack(args))
else
return
end
end
end
dig(tonumber(txt))
end)

NUKE:NewButton("stop auto Mine", "stop auto", function()
getgenv().digg = false
end)

NUKE:NewTextBox("Auto Train", "1-9 (WORK WITH THE MAP YOU ALREADY BUY)", function(txt)
getgenv().trains = true
function train(num1)
while wait(0.01) do
    if trains then
local args = {
    [1] = num1
}

game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Mine"):WaitForChild("AddTrain"):FireServer(unpack(args))
else
return
end
end
end
train(tonumber(txt))
end)

NUKE:NewButton("stop auto Train", "stop auto", function()
getgenv().trains = false
end)
NUKE:NewTextBox("Auto win", "need insta mine block first", function(txt)
getgenv().wins = true
function win(num4)
while wait(0.1) do
    if wins then
local args = {
    [1] = num4
}

game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Mine"):WaitForChild("CheckIfWin"):FireServer(unpack(args))
else
return
end
end
end
win(tonumber(txt))
end)

NUKE:NewButton("stop auto win", "stop auto ", function()
getgenv().wins = false
end)
NUKE:NewTextBox("1 Egg open", "1-18", function(txt)
function egg(num3)
local args = {
    [1] = num3,
    [2] = "Open1",
    [3] = {}
}

game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Hatch"):WaitForChild("EggHatch"):InvokeServer(unpack(args))
end
egg(tonumber(txt))
end)

NUKE:NewTextBox("3 Egg open", "1-18", function(txt)
function eggs(num2)
local args = {
    [1] = num2,
    [2] = "Open3",
    [3] = {}
}

game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Hatch"):WaitForChild("EggHatch"):InvokeServer(unpack(args))
end
eggs(tonumber(txt))
end)

