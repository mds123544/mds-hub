NUKE:NewTextBox("Map tp", "1-9", function(txt)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Mine[txt].Block["1"].CFrame + Vector3.new(0, 15, 0)
end)
SettingSection:NewKeybind("Toggle UI", "Toggle key to enable/off gui", Enum.KeyCode.F, function()
	Library:ToggleUI()
end)
